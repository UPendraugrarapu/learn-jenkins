pipeline {
  agent {
    node {label 'workstation'}
  }
  parameters {
          string(name: 'PERSON', defaultValue: 'Mr Jenkins', description: 'Who should I say hello to?')
          }
  environment {
    SAMPLE_URL = 'google.com'
    SSH = credentials("SSH")
  }

  stages {
    stage ("one"){
      steps{
          echo "one"
          sh 'env'
          }
      }

    stage ("two"){
     steps{
      echo "two"
     }
    }
  }
   post{
    failure{
      echo 'test post action'
    }
   }
}