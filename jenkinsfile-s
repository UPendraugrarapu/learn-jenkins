node('workstation') {
  try{
    stage ("one"){
       echo "one"
        sh 'exit 1'
          }


    stage ("two"){
     echo "two"
     }
  } catch(e) {
         stage('Declarative : post actions') {
            echo 'this will be run if any failed'
              }
  }
}

##catch(e) stands for exception